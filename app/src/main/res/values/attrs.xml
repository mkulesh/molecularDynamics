<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="colorInfoText" format="color" />
    <attr name="colorInfoTextShadow" format="color" />
    <attr name="panelBackground" format="reference" />

    <declare-styleable name="ImageListPreference">
        <attr name="entryImages" format="reference" />
    </declare-styleable>
    
    <declare-styleable name="PotentialView">
        <attr name="domainName" format="string" />
        <attr name="valueName" format="string" />
        <attr name="axisColor" format="color" />
        <attr name="axisWidth" format="dimension" />
        <attr name="axisArrowSize" format="dimension" />
        <attr name="axisLabelSize" format="dimension" />
        <attr name="plotLineColor" format="color" />
        <attr name="plotLineWidth" format="dimension" />
        <attr name="formulaWidth" format="float" />
        <attr name="labelNumber" format="integer" />
    </declare-styleable>

    <!-- AndroidSVG library -->
    <declare-styleable name="SVGImageView">
        <!-- Location of the SVG document. -->
        <attr name="svg" format="reference|string"/>
        <attr name="css" format="string"/>
    </declare-styleable>

    <declare-styleable name="VerticalSeekBar">
        <attr name="customThumb" format="reference"/>
    </declare-styleable>

</resources>